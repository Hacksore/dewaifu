{
  "background": { "scripts": ["background/index.ts"] },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": ["content/index.ts"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "browser_action": { "default_popup": "pages/popup/index.html" },
  "permissions": ["storage"]
}
